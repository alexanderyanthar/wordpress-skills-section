{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/skills-section",
	"version": "0.1.0",
	"title": "Skills Section",
	"category": "media",
	"icon": "editor-ul",
	"description": "A block to list your developer skills",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"skillTitle": {
			"type": "string",
			"source": "html",
			"selector": "h2"
		},
		"selectedIcons": {
			"type": "array",
			"default": []
		}
	},
	"textdomain": "skills-section",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
